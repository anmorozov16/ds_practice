   "source": [
    "import statsmodels.stats.power as smp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "id": "6162d26f",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
