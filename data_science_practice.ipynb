    "def swap_rows(row1, row2):\n",
    "    lst_index = list(df.index)\n",
    "    a, b = lst_index.index(row1), lst_index.index(row2) \n",
    "    lst_index[a], lst_index[b] = lst_index[b], lst_index[a]\n",
    "    return lst_index\n",
    "\n",
    "df = df.reindex(swap_rows(1,2)\n",
    "               )\n",
    "df"
   ]
