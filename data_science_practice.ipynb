    "# 24. How to filter words that contain atleast 2 vowels from a series?\n",
    "\n",
    "ser = pd.Series(['Apple', 'Orange', 'Plan', 'Python', 'Money'])\n",
    "\n",
    "vowels = 'aeiou'\n",
    "vowels = vowels+vowels.upper()\n",
    "lst_filt = []\n",
    "for n in ser:\n",
    "    count = 0\n",
    "    for i in vowels:\n",
    "        if i in n:\n",
    "            count += 1\n",
    "    if count >= 2:\n",
    "        lst_filt.append(n)\n",
    "\n",
    "lst_filt"
   ]
